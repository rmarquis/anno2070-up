              <SLOT>Slots_ExtentChanged</SLOT>
              <SCRIPTABLE/>
              <SCRIPT><![CDATA[local addButton = arg.Sender:GetWindow("../ADD_SLOT");
if (addButton) then	
	local numVisibleSlots = 0;
	local position = addButton.Position;
	position.Y = arg.Sender.Extent.bottom - 2;

	for child in arg.Sender.Children do
		if child.Visible then
			numVisibleSlots = numVisibleSlots + 1;
		end
	end

	local connections = arg.Sender:GetWindow("../CONNECTIONS");
	local i = 0;

	if (connections ~= nil) then
		for child in connections.Children do
			child.Visible = i < numVisibleSlots;
			i = i + 1;
		end

		if (numVisibleSlots < 3) then
			addButton:StopAllFx();
			if (numVisibleSlots == 0) then
				addButton:StartFx(FxMove(PointF(position.X, position.Y+2), Fx.EaseInOut, 0.1, addButton));
			else
				addButton:StartFx(FxMove(PointF(position.X, position.Y-2), Fx.EaseInOut, 0.1, addButton));
			end
			addButton:StartFx(FxAlpha(1, Fx.EaseInOut, 0.2, addButton));
		else
			addButton:StopAllFx();
			addButton:StartFx(FxAlpha(0, Fx.EaseInOut, 0.2, addButton));
		end
	end
end

]]></SCRIPT>
