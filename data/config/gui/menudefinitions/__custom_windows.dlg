      <ACTION>
        <NAME>HighlightPoints</NAME>
        <PARAMETERS><![CDATA[int team]]></PARAMETERS>
        <SCRIPT><![CDATA[-- select correct points label
local activePointsLabel = nil;

if (team == DataSetBindings.CONQUESTTEAM_TEAM1) then
	activePointsLabel = window:GetWindow("BG_CARDS/MISSION_PANEL/POINT_PANEL/TEAM1");
elseif (team == DataSetBindings.CONQUESTTEAM_TEAM2) then
	activePointsLabel = window:GetWindow("BG_CARDS/MISSION_PANEL/POINT_PANEL/TEAM2");
elseif (team == DataSetBindings.CONQUESTTEAM_TEAM3) then
	activePointsLabel = window:GetWindow("BG_CARDS/MISSION_PANEL/POINT_PANEL/TEAM3");
elseif (team == DataSetBindings.CONQUESTTEAM_TEAM4) then
	activePointsLabel = window:GetWindow("BG_CARDS/MISSION_PANEL/POINT_PANEL/TEAM4");
end

if (activePointsLabel ~= nil) then
	local fx = FxCollection(FxCollection.PlaySerial);
	local delay = 0.15;

	fx:AddFx(FxAlpha(0.0, Fx.EaseInOutSine, 0.0, activePointsLabel));
	fx:AddFx(FxDelay(delay));
	fx:AddFx(FxAlpha(1.0, Fx.EaseInOutSine, 0.0, activePointsLabel));
	fx:AddFx(FxDelay(delay));
	fx:AddFx(FxAlpha(0.0, Fx.EaseInOutSine, 0.0, activePointsLabel));
	fx:AddFx(FxDelay(delay));
	fx:AddFx(FxAlpha(1.0, Fx.EaseInOutSine, 0.0, activePointsLabel));

	activePointsLabel:StartFx(fx);
end]]></SCRIPT>
