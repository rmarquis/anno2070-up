if (this.WareType == 1) then
	amountValueWnd = window:GetWindow("WARE_TYPE/WARE/WARE_BUTTON/AMOUNT_VALUE");
elseif (this.WareType == 2) then
	amountValueWnd = window:GetWindow("WARE_TYPE/NO_MAX_WARE/WARE_BUTTON/AMOUNT_VALUE");
else
	amountValueWnd = window:GetWindow("WARE_TYPE/FARM_FIELD_BUTTON/AMOUNT_VALUE");
end

if (window.ShowMaxAmount) then
	local maxAmount = window.MaxAmount;

	if (amount == maxAmount and
		this.WareType == 3) then
		amountValueWnd.Text = "<positivevaluedark>"..FormatNumber(amount, false).."/"..FormatNumber(maxAmount, false).."</positivevaluedark>";
	else
		amountValueWnd.Text = FormatNumber(amount, false).."/"..FormatNumber(maxAmount, false);
