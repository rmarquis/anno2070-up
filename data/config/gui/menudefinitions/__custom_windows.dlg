fx:AddFx(FxSize(SizeF(newWidth, window.Height), Fx.EaseInExpo, 0.2, window, false));
fx:AddFx(FxCallback(ChangeSizeCallback, window));
window:StartFx(fx);

window:UpdateText();]]></SETTER>
