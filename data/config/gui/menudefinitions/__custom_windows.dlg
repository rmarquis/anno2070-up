	if (wnd.Collapsed) then
		timerBoxWnd.AutoSizeOrientation = CGUIWnd.ORIENTATION_BOTH;
		timerBoxWnd.LayoutModeH = CGUIWnd.LayoutModeH_None;
		timerPanelWnd.AutoSizeOrientation = CGUIWnd.ORIENTATION_BOTH;
