        <PARAMETERS><![CDATA[bool useGUIFx]]></PARAMETERS>
        <SCRIPT><![CDATA[local contentSize = SizeF(0, 0);
local childFound = false;

window:PreRender();

for child in window.Children do
	if (child.PathID ~= "__INNER_PANEL") then
		contentSize = SizeF(math.max(contentSize.Width, child.Position.X+child.Size.X),
							math.max(contentSize.Height, child.Position.Y+child.Size.Y));
		childFound = true;
	end
end

if (childFound) then
	local desiredSize;
	local arguments = CGUIEventArg();
	arguments.Sender = window:GetWindow("");

	if (window.Collapsed) then
		desiredSize = SizeF(window.Size.X, window.TitleHeight);
	else
		desiredSize = SizeF(window.Size.X, contentSize.Height);
	end

	window:StopAllFx();
	if (useGUIFx) then
		window.BeginUpdateSize:Call(arguments);
		local fx = FxCollection(FxCollection.PlaySerial);
		fx:AddFx(FxSize(desiredSize, Fx.Linear, 0.15, window, false));
		fx:AddFx(FxCallback(function(wnd) wnd.SizeUpdated:Call(arguments); end, window));
		window:StartFx(fx);
	else
		window.BeginUpdateSize:Call(arguments);
		window.Size = Size(desiredSize.Width, desiredSize.Height);
		window.SizeUpdated:Call(arguments);
	end
end
