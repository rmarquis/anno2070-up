      <CONSTRUCTOR><![CDATA[local function itemLClickInternalHandler(modelWindow, arg)
	window:GetWindow("CONFIRMATION_MASK/CONFIRMATION_PANEL").Visible = true;
end

local function itemClickHandler(modelWindow, arg)
	if (arg.Sender:IsLButtonDown() and modelWindow.IsActivatable) then
		if (modelWindow.RequiresConfirmationOnActivation and
			GetGUIItemManager():CanUseItem(modelWindow.SocketIndex, modelWindow.TargetHID)) then
			modelWindow.ShowActivationPrompt:Call(arg);
			modelWindow.ItemLClick_Internal:Call(arg);
		else
			modelWindow.ItemLClick:Call(arg);
		end
	elseif (arg.Sender:IsRButtonDown()) then
		modelWindow.ItemRClick:Call(arg);
	end
end

local function itemDragEndedHandler(modelWindow, arg)
	modelWindow.ItemDragEnded:Call(arg);
end

