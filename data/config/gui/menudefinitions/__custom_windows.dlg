		timerValueWnd.Left = 0;
		timerValueWnd.LayoutModeH = CGUIWnd.LayoutModeH_None;
		wnd.AutoSizeOrientation = CGUIWnd.ORIENTATION_BOTH;
	end
end

this.Collapsed = value;

local newWidth = 0;
if (value) then
	this.ParentOriginalWidth = window.Parent.Width;
	newWidth = window:GetWindow("TIMER_BOX/TIMER_PANEL/TIMER_VALUE").Width;
else
	newWidth = this.ParentOriginalWidth;

	local timerBoxWnd = window:GetWindow("TIMER_BOX");
	local timerPanelWnd = timerBoxWnd:GetWindow("TIMER_PANEL");
	timerBoxWnd.AutoSizeOrientation = CGUIWnd.ORIENTATION_VERTICAL;
	timerPanelWnd.AutoSizeOrientation = CGUIWnd.ORIENTATION_VERTICAL;
