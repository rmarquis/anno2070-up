            <SLOT>ConfirmationPanel_Create</SLOT>
            <SCRIPTABLE/>
            <SCRIPT><![CDATA[local function onConfirmationClickOK(confirmationPanel, arg)
	confirmationPanel.Parent.Parent.RequiresConfirmationOnActivation = false;
	confirmationPanel.Parent.Parent.ItemLClick:Call(arg);
end

local function onConfirmationClickCancel(confirmationPanel, arg)
	confirmationPanel.Parent.Parent.RequiresConfirmationOnActivation = false;
end

local okButton = arg.Sender:GetWindow("OK_BUTTON");
okButton.MouseClick:Clear();
okButton.MouseClick:AddSlot(arg.Sender, onConfirmationClickOK);

local cancelButton = arg.Sender:GetWindow("CANCEL_BUTTON");
cancelButton.MouseClick:Clear();
cancelButton.MouseClick:AddSlot(arg.Sender, onConfirmationClickCancel);]]></SCRIPT>
          </EVENT>
          <EVENT>
            <TYPE>MouseUp</TYPE>
            <SLOT>ConfirmationPanel_MouseUp</SLOT>
            <SCRIPTABLE/>
            <SCRIPT><![CDATA[if (arg.Sender.Visible and not arg.Sender:GetExtentAbsolute():PointInRect(GUI.GetCursor().Position)) then
	arg.Sender.Visible = false;
end]]></SCRIPT>
          </EVENT>
          <EVENT>
            <TYPE>Hide</TYPE>
            <SLOT>ConfirmationPanel_Hide</SLOT>
            <SCRIPTABLE/>
            <SCRIPT><![CDATA[arg.Sender.Visible = false;]]></SCRIPT>
          </EVENT>
          <EVENT>
            <TYPE>Tick</TYPE>
            <SLOT>ConfirmationPanel_Tick</SLOT>
            <SCRIPTABLE/>
            <SCRIPT><![CDATA[local modelWindow = arg.Sender.Parent.Parent;
