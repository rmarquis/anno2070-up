	window:UpdateSize(false);
end
]]></SCRIPT>
      </ACTION>
      <ACTION>
        <NAME>UpdateSize</NAME>
        <PARAMETERS><![CDATA[bool useGUIFx
]]></PARAMETERS>
        <SCRIPT><![CDATA[local contentSize = SizeF(0, 0);
local childFound = false;
local dynamicContent = window:GetWindow("__INNER_PANEL/PRODUCING_BUILDINGS");

dynamicContent:PreRender();

for child in dynamicContent.Children do
	contentSize = SizeF(math.max(contentSize.Width, child.Position.X+child.Size.X),
						math.max(contentSize.Height, child.Position.Y+child.Size.Y));
	childFound = true;
end

if (childFound) then
	local desiredSize;

	if (window.Collapsed) then
		desiredSize = SizeF(window.Size.X, 0);
	else
		desiredSize = SizeF(window.Size.X, contentSize.Height);
	end

	dynamicContent:StopAllFx();
	if (useGUIFx) then
		dynamicContent:StartFx(FxSize(desiredSize, Fx.Linear, 0.15, dynamicContent, false));
	else
		dynamicContent.Size = Size(desiredSize.Width, desiredSize.Height);
	end
end
]]></SCRIPT>
