      <CONSTRUCTOR><![CDATA[this.ShowBars = true;
this.Active = true;

local function SettingsSelectionChanged(arg)
	local barList = arg.Sender:GetWindow("../BARS");
	local i = 0;
	
	for bar in barList.Children do
		local size = bar.Size;
		
		if (i <= arg.Sender.SelectedIndex) then
			bar:UseTemplate("GAME_SETTING_CHOICE_BAR_FILLED");
			bar.Size = Size(size.X, 0);
		else
			bar:UseTemplate("GAME_SETTING_CHOICE_BAR");
			bar.Size = Size(size.X, 0);
		end
		
		i = i +1;
	end
end

window:GetWindow("BUTTON/COMBOBOX").SelectionChanged:AddSlot(SettingsSelectionChanged);]]></CONSTRUCTOR>
