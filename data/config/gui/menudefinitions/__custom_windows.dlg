      <ACTION>
        <NAME>UpdateButtonCards</NAME>
        <PARAMETERS/>
        <SCRIPT><![CDATA[local cardsWnd = window:GetWindow("BOX/PANEL/BUTTON_CARDS");
local showTimeLeft = true;
local showEndDate = true;
local showEndedPanel = false;
local showTimePerDay = true;
local showScore = true;
local showPlayers = false;

if (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_PENDING) then
	showTimeLeft = false;
	showScore = false;
elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ACTIVE or
		this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ENDING) then
	-- show everything
elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_SUDDENDEATH) then
	showEndDate = false;
elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ENDED) then
	showTimeLeft = false;
	showEndDate = false;
	showEndedPanel = true;
end

if (window.PublicMode) then
	cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_PUBLIC");
	showPlayers = true;
	showTimeLeft = false;
elseif (window.InvitationMode) then
	cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_INVITED");
	showPlayers = true;
	showTimeLeft = false;
else
	if (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_PENDING) then
		cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_PENDING");
	elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ACTIVE or
			this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ENDING) then
		cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_ACTIVE");
	elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_SUDDENDEATH) then
		cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_ACTIVE");
	elseif (this.Mode == DataSetBindings.CONQUESTSESSIONSTATES_ENDED) then
		cardsWnd.ActivatedChild = cardsWnd:GetWindow("BOX_ENDED");
	end
end

window:GetWindow("BOX/PANEL/TIMELEFT_PANEL").Visible = showTimeLeft;
window:GetWindow("BOX/PANEL/ENDDATE_PANEL").Visible = showEndDate;
window:GetWindow("BOX/PANEL/ENDED_PANEL").Visible = showEndedPanel;
window:GetWindow("BOX/PANEL/TIMEPERDAY_PANEL").Visible = showTimePerDay;
window:GetWindow("BOX/PANEL/SCORE_PANEL").Visible = showScore;
window:GetWindow("BOX/PANEL/PLAYERS_PANEL").Visible = showPlayers;]]></SCRIPT>
