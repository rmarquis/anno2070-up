      <ACTION>
        <NAME>UpdateLayout</NAME>
        <PARAMETERS/>
        <SCRIPT><![CDATA[window:GetWindow("WARE_TYPE/WARE_ADD/UP").Toggled = (this.PassiveWareBuy == CTradeAction.TRADETYPE_SELL);
window:GetWindow("WARE_TYPE/WARE_ADD/DOWN").Toggled = (this.PassiveWareBuy == CTradeAction.TRADETYPE_BUY);

if (this.PassiveWareBuy == CTradeAction.TRADETYPE_SELL) then
	if (this.PassiveWareLayout == 0) then
		window:GetWindow("WARE_TYPE/WARE_BUTTON"):UseTemplate("WAREHOUSE_PASSIVE_WARE_PANEL_SELL");
	else
		window:GetWindow("WARE_TYPE/WARE_BUTTON"):UseTemplate("WAREHOUSE_PASSIVE_WARE_PRICE_PANEL_SELL");
	end
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_SLIDER"):UseTemplate("WAREHOUSE_AMOUNT_SLIDER_RED");
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_BAR"):UseTemplate("WAREHOUSE_AMOUNT_BAR_LIGHT_RED");
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_BG_BAR"):UseTemplate("WAREHOUSE_AMOUNT_BAR_DARK_RED");
	window:GetWindow("BUY_SELL_LAYOUT").ActivatedChild = window:GetWindow("BUY_SELL_LAYOUT/SELL_PANEL");
else
	if (this.PassiveWareLayout == 0) then
		window:GetWindow("WARE_TYPE/WARE_BUTTON"):UseTemplate("WAREHOUSE_PASSIVE_WARE_PANEL");
	else
		window:GetWindow("WARE_TYPE/WARE_BUTTON"):UseTemplate("WAREHOUSE_PASSIVE_WARE_PRICE_PANEL");
	end
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_SLIDER"):UseTemplate("WAREHOUSE_AMOUNT_SLIDER_GREEN");
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_BAR"):UseTemplate("WAREHOUSE_AMOUNT_BAR_LIGHT_GREEN");
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT/AMOUNT_BG_BAR"):UseTemplate("WAREHOUSE_AMOUNT_BAR_DARK_GREEN");
	window:GetWindow("BUY_SELL_LAYOUT").ActivatedChild = window:GetWindow("BUY_SELL_LAYOUT/BUY_PANEL");
end

if (this.PassiveWareLayout == 0) then
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT").ActivatedChild = window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/AMOUNT");
else
	window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT").ActivatedChild = window:GetWindow("WARE_TYPE/WARE_BUTTON/PASSIVE_LAYOUT/PRICE");
end]]></SCRIPT>
