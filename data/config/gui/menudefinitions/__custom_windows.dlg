elseif (this.Mode == CSessionPlayer.PS_PUBLIC) then
	tooltipTextID = 1121388 + GUI.Constants.TEXTDEF_TOOLTIPDEFAULTMIN;
end

for child in window:GetWindow("BUTTON_BGS").Children do
	child.TooltipText = tooltipTextID;
end]]></SCRIPT>
