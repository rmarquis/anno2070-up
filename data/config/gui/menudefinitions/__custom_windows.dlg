else
	bodyBg = "(invalid)";
	reputationProgressBg = "(invalid)";
end

if (this.RoundedBackground) then
	if (this.EnableBigLayout) then
		bodyBg = bodyBg .. "_rounded.bkg"
	else
		bodyBg = bodyBg .. "_rounded_small.bkg"
	end
else
	bodyBg = bodyBg .. ".bkg"
end

window:GetWindow("BODY").Background = bodyBg;
