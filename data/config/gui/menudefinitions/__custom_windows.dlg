      </EVENT>
      <ATTRIBUTE>
        <NAME>SelectedLevel</NAME>
        <TYPE>0</TYPE>
        <GETTER><![CDATA[return this.SelectedLevel;]]></GETTER>
        <SETTER><![CDATA[this.SelectedLevel = value;
local detailInformation = window:GetWindow("BODY/DETAIL_INFORMATION");

-- find selected level window
local reputationBox = window:GetWindow("BODY/REPUTATION_BOX");
local levelWnd = nil;
local levelCount = 0;
for child in reputationBox.Children do
	if (child.ZOrder == this.SelectedLevel) then
		child.Selected = true;
		levelWnd = child;
	else
		child.Selected = false;
	end
	
	if (child.Visible) then
		levelCount = levelCount + 1;
	end
end


if (this.SelectedLevel < 0) then
	local desiredSize = SizeF(detailInformation.Size.X, 0.0);
	detailInformation:StartFx(FxSize(desiredSize, Fx.Linear, 0.15, detailInformation, false));

	window:GetWindow("BODY/CONNECTION_PANEL").Visible = false;
else
	local desiredSize = SizeF(detailInformation.Size.X, 269.0);
	detailInformation:StartFx(FxSize(desiredSize, Fx.Linear, 0.15, detailInformation, false));

	local connectionPanel = window:GetWindow("BODY/CONNECTION_PANEL");
	connectionPanel.Visible = true;
	
	
	-- set connection panel extent based on the current level
	if (levelWnd ~= nil) then
		if (this.SelectedLevel < levelCount/2) then
			connectionPanel.Background = "data/config/gui/backgrounds/prototype/mainmenu_frames/mainmenu_vertical_connection_left.bkg";
		else
			connectionPanel.Background = "data/config/gui/backgrounds/prototype/mainmenu_frames/mainmenu_vertical_connection_right.bkg";
		end
	
		local levelWndExtent = levelWnd.Extent;
		local connectionWndExtent = connectionPanel.Extent;
		
		connectionWndExtent.left = reputationBox.Left + levelWndExtent.left + levelWndExtent:Width() / 2;
		connectionWndExtent.right = 500;
		
		if (connectionWndExtent.left > connectionWndExtent.right) then
			connectionWndExtent.left, connectionWndExtent.right = connectionWndExtent.right, connectionWndExtent.left;
		end
	
		connectionPanel.Extent = connectionWndExtent;
	end
	
	-- set level progress
	window.DetailMaxProgress = levelWnd.MaxProgress;
	window.DetailProgress = levelWnd.Progress;
	
	local progressTextWnd = window:GetWindow("BODY/DETAIL_INFORMATION/FRAME/PROGRESS_TEXT");
	local progressBarWnd = window:GetWindow("BODY/DETAIL_INFORMATION/FRAME/REPUTATION_PROGRESS");
	if (levelWnd.Layout == 0) then
		--completed
		progressTextWnd.TextGUID = 1007753;
		progressBarWnd.Visible = true;
	elseif (levelWnd.Layout == 1) then
		--in_progress
		progressTextWnd.TextGUID = 1007752;
		progressBarWnd.Visible = true;
	else
		--impossible
		progressTextWnd.TextGUID = 1007754;
		progressBarWnd.Visible = true;
	end
end]]></SETTER>
