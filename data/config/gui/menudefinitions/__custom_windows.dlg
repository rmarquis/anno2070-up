      <ACTION>
        <NAME>UpdateButtonState</NAME>
        <PARAMETERS/>
        <SCRIPT><![CDATA[local buttonCards = window:GetWindow("BG_CARDS");

local bgWnd = nil;
local buttonWnd = nil;

-- mission or home buttons?
if (window.HomeButton) then
	buttonCards.ActivatedChild = buttonCards:GetWindow("HOME_BGS");
	bgWnd = buttonCards.ActivatedChild;
else
	buttonCards.ActivatedChild = buttonCards:GetWindow("MISSION_PANEL");
	bgWnd = buttonCards:GetWindow("MISSION_PANEL/MISSION_BGS");
	buttonWnd = buttonCards:GetWindow("MISSION_PANEL/MISSION_BUTTONS");
end

local wndName;

-- select correct team layout
if (window.Owner == DataSetBindings.CONQUESTTEAM_TEAM1) then
	wndName = "TEAM1";
elseif (window.Owner == DataSetBindings.CONQUESTTEAM_TEAM2) then
	wndName = "TEAM2";
elseif (window.Owner == DataSetBindings.CONQUESTTEAM_TEAM3) then
	wndName = "TEAM3";
elseif (window.Owner == DataSetBindings.CONQUESTTEAM_TEAM4) then
	wndName = "TEAM4";
else
	wndName = "NEUTRAL";
end

if (bgWnd ~= nil) then
	bgWnd.ActivatedChild = bgWnd:GetWindow(wndName);
end

if (buttonWnd ~= nil) then
	buttonWnd.ActivatedChild = buttonWnd:GetWindow(wndName);
end
]]></SCRIPT>
