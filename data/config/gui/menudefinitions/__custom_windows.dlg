      <ACTION>
        <NAME>SetPlayerInfo</NAME>
        <PARAMETERS><![CDATA[const CRDString& portraitFilename, const CRDStringW& playerName, int statusGUID, const CRDStringW& title, int score]]></PARAMETERS>
        <SCRIPT><![CDATA[local activePanel = nil;
local nameWnd = nil;
local portraitWnd = nil;
local statusWnd = nil;
local titleWnd = nil;
local scoreWnd = nil;

if (this.Mode == CSessionPlayer.PS_PLAYING) then
	activePanel = window:GetWindow("LAYOUT/ACTIVE");
elseif (this.Mode == CSessionPlayer.PS_EMPTY) then
	activePanel = window:GetWindow("LAYOUT/EMPTY");
elseif (this.Mode == CSessionPlayer.PS_INVITED) then
	activePanel = window:GetWindow("LAYOUT/INVITED");
elseif (this.Mode == CSessionPlayer.PS_PUBLIC) then
	activePanel = window:GetWindow("LAYOUT/PUBLIC");
end

if (activePanel) then
	nameWnd = activePanel:GetWindow("NAME");
	portraitWnd = activePanel:GetWindow("PORTRAIT");
	statusWnd = activePanel:GetWindow("STATUS");
	titleWnd = activePanel:GetWindow("TITLE");
	scoreWnd = activePanel:GetWindow("PORTRAIT/SCORE");
end

if (nameWnd) then
	nameWnd.Text = playerName;
end
if (portraitWnd) then
	if (this.Mode == CSessionPlayer.PS_INVITED) then
		activePanel:GetWindow("PENDING_VIDEO").Visible = window.ActiveInvitation;
		portraitWnd.Visible = not window.ActiveInvitation;
	else
		portraitWnd.Background = portraitFilename;
	end
end
if (statusWnd) then
	statusWnd.TextGUID = statusGUID;
end
if (titleWnd) then
	titleWnd.Text = title;
end
if (scoreWnd) then
	scoreWnd.Text = tostring(score);
end



-- update text info
local playerButton = window:GetWindow("BUTTON_BGS").ActivatedChild;
local textInfo = TEXTINFOSTRUCT();
textInfo.PlayerID = window.Team;
textInfo.PlayerID2 = window.PlayerIndex;
playerButton.TooltipData = textInfo;]]></SCRIPT>
