      <ACTION>
        <NAME>UpdateState</NAME>
        <PARAMETERS/>
        <SCRIPT><![CDATA[local button = window:GetWindow("BUTTON");
local boxlayout = button:GetWindow("BOXLAYOUT");
local cleared = boxlayout:GetWindow("PICTURE/CLEARED");
local clearedBig = boxlayout:GetWindow("INFO/CLEARED");
local difficultyBar = boxlayout:GetWindow("PICTURE/LABEL/DIFFICULTY_BAR");

local defaultColor = HsvaColor();
local disabledColor = HsvaColor();
disabledColor.S = 0;

-- enabled
if (window.State == 0) then
	boxlayout.Color = defaultColor;
	difficultyBar.Color = defaultColor;
	button.Enabled = true;
	cleared.Visible = false;
	clearedBig.Visible = false;
-- disabled
elseif (window.State == 1) then
	boxlayout.Color = disabledColor;
	difficultyBar.Color = disabledColor;
	button.Enabled = false;
	cleared.Visible = false;
	clearedBig.Visible = false;
-- cleared
elseif (window.State == 2) then
	boxlayout.Color = defaultColor;
	difficultyBar.Color = defaultColor;
	button.Enabled = true;
	cleared.Visible = true;
	cleared.Alpha = 1.0;
	clearedBig.Visible = true;
-- cleared disabled
elseif (window.State == 3) then	
	boxlayout.Color = disabledColor;
	difficultyBar.Color = disabledColor;
	button.Enabled = false;
	cleared.Visible = true;
	cleared.Alpha = 1.0;
	clearedBig.Visible = true;
end]]></SCRIPT>
