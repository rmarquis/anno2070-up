      <ACTION>
        <NAME>UpdateState</NAME>
        <PARAMETERS/>
        <SCRIPT><![CDATA[local button = window:GetWindow("BUTTON");
local medal = button:GetWindow("MEDAL");
local label = button:GetWindow("NAME");
local description = button:GetWindow("DESCRIPTION");

-- not cleared
if (this.State == 0) then
	medal.Alpha = 0.5;
	button.Enabled = false;
	local color = HsvaColor();
	color.S = 0;
	button.Color = color;
	label.StyleSelector = "";
	label:SelectStyle("achievementDisabled");
	label.TextColor = RgbColor(200,200,200);
	description.TextColor = RgbColor(200,200,200);
-- silver
elseif (this.State == 1) then
	medal.Alpha = 1;
	button.Enabled = true;
	button.Color = HsvaColor();
	label:SelectStyle("gradientHeadline");
	description.TextColor = RgbColor(255,255,255);
-- gold
else
	medal.Alpha = 1;
	button.Enabled = true;
	button.Color = HsvaColor();
	label:SelectStyle("gradientHeadline");
	description.TextColor = RgbColor(255,255,255);
end

-- set medal for silver and uncleared
if (this.State == 0) then
	if (this.Type == 0) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_normal_disabled.bkg";
	elseif (this.Type == 1) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_expert_disabled.bkg";
	elseif (this.Type == 2) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_hidden_disabled.bkg";
	end
-- set medal for gold
else
	if (this.Type == 0) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_normal.bkg";
	elseif (this.Type == 1) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_expert.bkg";
	elseif (this.Type == 2) then
		medal.Background = "data/config/gui/backgrounds/prototype/common_frames/common_achievement_hidden.bkg";
	end
end
]]></SCRIPT>
