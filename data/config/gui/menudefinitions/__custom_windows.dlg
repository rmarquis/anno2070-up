      <CONSTRUCTOR><![CDATA[local function ButtonClickHandler(modelWindow, arg)
	if (arg.Button == CGUIMouseEventArg.MOUSE_BTN_LEFT) then
		modelWindow.ButtonLClick:Call(arg);
	else
		modelWindow.ButtonRClick:Call(arg);
	end
end

--------------------------------------------

local buttonCards = window:GetWindow("BUTTON_BGS");
buttonCards:GetWindow("TEAM1").MouseClick:AddSlot(window, ButtonClickHandler);
buttonCards:GetWindow("TEAM2").MouseClick:AddSlot(window, ButtonClickHandler);
buttonCards:GetWindow("TEAM3").MouseClick:AddSlot(window, ButtonClickHandler);
buttonCards:GetWindow("TEAM4").MouseClick:AddSlot(window, ButtonClickHandler);

window.PlayerIndex = -1;]]></CONSTRUCTOR>
