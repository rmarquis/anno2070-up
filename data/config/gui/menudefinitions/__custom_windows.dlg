            <SLOT>AddTaskBtn_Click</SLOT>
            <SCRIPTABLE/>
            <SCRIPT><![CDATA[local taskDescription = arg.Sender.Parent:GetChild("TASK_NAME_FIELD").Text;

if (#taskDescription > 0 ) then
	local todoList = arg.Sender.Parent.Parent;
	local todoEntry = todoList.EntryList:CreateCustomChild("TODO_ENTRY");
	todoEntry.Visible = true;
	todoEntry.AutoSize = true;
	todoEntry.AutoSizeOrientation = CGUIWnd.ORIENTATION_BOTH;
	todoEntry.Description = taskDescription;
	todoEntry.Priority = 1;
	todoEntry.TaskStateChange:AddSlot(todoList, 
		function (todoList, arg) 
			if (todoList.SortType == 0) then
				todoList:Sort();
			end
		end);
	arg.Sender.Parent:GetChild("TASK_NAME_FIELD").Text = "";
end
]]></SCRIPT>
