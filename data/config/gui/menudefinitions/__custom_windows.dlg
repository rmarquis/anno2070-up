      </ATTRIBUTE>
      <ACTION>
        <NAME>UpdateState</NAME>
        <SCRIPT><![CDATA[local button = window:GetWindow("BUTTON");
local title = button:GetWindow("BOXLAYOUT/INFO/TITLE");
local name = button:GetWindow("BOXLAYOUT/INFO/NAME");
local picture = button:GetWindow("BOXLAYOUT/PICTURE");
local anchor = button:GetWindow("BOXLAYOUT/ANCHOR");

local defaultColor = HsvaColor();
local disabledColor = HsvaColor();

disabledColor.H = 0;
disabledColor.S = 0;
disabledColor.V = 0.6;

-- enabled
if (window.State == 0) then
	button.Enabled = true;
	picture.Color = defaultColor;
		
	title.Alpha = 1;
	name.Alpha = 1;
	anchor.Background = "data/config/gui/backgrounds/prototype/common_frames/common_buttonanchor.bkg";
	
-- disabled
elseif (window.State == 1) then
	button.Enabled = false;
	picture.Color = disabledColor;
		
	title.Alpha = 0.5;
	name.Alpha = 0.5;
	anchor.Background = "data/config/gui/backgrounds/prototype/common_frames/common_buttonanchor_disabled.bkg";
	
end
]]></SCRIPT>
