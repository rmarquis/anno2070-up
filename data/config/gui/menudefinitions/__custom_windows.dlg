      <CONSTRUCTOR><![CDATA[local function onJoinPublicGame(modelWindow, arg)
	if (arg.ID == CMessageBoxManager.MB_RESULT_OK) then
		arg.Sender = modelWindow:GetWindow("");
		modelWindow.PublicJoin:Call(arg);
	end
end

local function smartClickHandler(modelWindow, arg)
	local buttonCardsWnd = modelWindow:GetWindow("BOX/PANEL/BUTTON_CARDS");

	arg.Sender = modelWindow:GetWindow("");
	if (buttonCardsWnd:GetWindow("BOX_ACTIVE").Visible) then
		modelWindow.ActiveContinue:Call(arg);
	elseif (buttonCardsWnd:GetWindow("BOX_ENDED").Visible) then
		modelWindow.EndedResults:Call(arg);
	--elseif (buttonCardsWnd:GetWindow("BOX_INVITED").Visible) then
	elseif (buttonCardsWnd:GetWindow("BOX_PENDING").Visible) then
		modelWindow.PendingContinue:Call(arg);
	elseif (buttonCardsWnd:GetWindow("BOX_PUBLIC").Visible) then
		GetMessageBoxManager():ShowMessageBox(1009750,
			1121409,
			CMessageBoxManager.MB_BTN_OK_CANCEL,
			CMessageBoxManager.MB_TYPE_DEFAULT,
			modelWindow,
			onJoinPublicGame);
	end
end

