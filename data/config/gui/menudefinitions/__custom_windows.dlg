socketDragPanel.DragOK:AddSlot(window, itemDragEndedHandler);

local socketButton = socketDragPanel:GetWindow("SOCKET_ITEM");
socketButton.MouseClick:AddSlot(window, itemClickHandler);

local actionButton = window:GetWindow("SOCKET_BACKGROUND/ACTION_ITEM");
actionButton.MouseClick:AddSlot(window, itemClickHandler);

Type = 0;
window.SocketQuality = 0;
window.Draggable = true;]]></CONSTRUCTOR>
