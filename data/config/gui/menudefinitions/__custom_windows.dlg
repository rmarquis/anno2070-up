      <ATTRIBUTE>
        <NAME>Team</NAME>
        <TYPE>0</TYPE>
        <GETTER><![CDATA[return this.Team;]]></GETTER>
        <SETTER><![CDATA[this.Team = value;

local wndName = "";
if (this.Team == DataSetBindings.CONQUESTTEAM_TEAM1) then
	wndName = "TEAM1";
elseif (this.Team == DataSetBindings.CONQUESTTEAM_TEAM2) then
	wndName = "TEAM2";
elseif (this.Team == DataSetBindings.CONQUESTTEAM_TEAM3) then
	wndName = "TEAM3";
elseif (this.Team == DataSetBindings.CONQUESTTEAM_TEAM4) then
	wndName = "TEAM4";
end

local buttonBGCards = window:GetWindow("BUTTON_BGS");
buttonBGCards.ActivatedChild = buttonBGCards:GetWindow(wndName);]]></SETTER>
      </ATTRIBUTE>
      <ATTRIBUTE>
        <NAME>Mode</NAME>
        <TYPE>0</TYPE>
        <GETTER><![CDATA[return this.Mode;]]></GETTER>
        <SETTER><![CDATA[this.Mode = value;

local cards = window:GetWindow("LAYOUT");
if (this.Mode == CSessionPlayer.PS_PLAYING) then
	cards.ActivatedChild = cards:GetWindow("ACTIVE");
elseif (this.Mode == CSessionPlayer.PS_EMPTY) then
	cards.ActivatedChild = cards:GetWindow("EMPTY");
elseif (this.Mode == CSessionPlayer.PS_INVITED) then
	cards.ActivatedChild = cards:GetWindow("INVITED");
elseif (this.Mode == CSessionPlayer.PS_PUBLIC) then
	cards.ActivatedChild = cards:GetWindow("PUBLIC");
end

window:UpdateTooltip();]]></SETTER>
